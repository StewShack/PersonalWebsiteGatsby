    "gatsby-source-filesystem": "^2.0.36",
    "gatsby-transformer-remark": "^2.3.12",
    "mocha": "^6.1.4",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.9",
